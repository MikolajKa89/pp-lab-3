package mainpackage;

import geometry.Square;

public class Main {
    public static void main(String[] args) {
        Square kwadrat = new Square(5.0);
        System.out.println("Długość boku: " + kwadrat.getDlugosc()); // korzystamy  metody z klasy Rectangle
        System.out.println("Pole: " + kwadrat.obliczPole()); // korzystamy  metody z klasy Rectangle
        System.out.println("Obwód: " + kwadrat.obliczObwod()); // korzystamyz  metody z klasy Rectangle
    }
}
